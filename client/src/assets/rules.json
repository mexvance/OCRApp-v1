    "flags": "i"
  },
  {
    "label": "8 Digits Only",
    "regex": "^[0-9]{8}$",
    "flags": "i"
  },
  {
    "label": "0+A+123456 Serial Format",
    "regex": "\\b\\d[A-Za-z]\\d{6}\\b",
    "flags": "i"
  },
  {
    "label": "FLOW text",
    "regex": "\\bFLOW\\s*\\d+\\s*[A-Za-z]?\\s*\\d+\\s*MM\\b",
    "flags": "i"
  }
  ]
  